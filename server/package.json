{
    "name": "server",
    "version": "1.0.0",
    "description": "CTF",
    "main": "dist",
    "scripts": {
        "build": "tsc -b",
        "server_watch_TS": "tsc --watch",
        "server_with_auto_reload_files": "nodemon dist/server.js",
        "dev_backend" : "concurrently --kill-others  \"npm run server_watch_TS \" \"npm run server_with_auto_reload_files\"",
        "server_dev": " npm run build && npm run dev_backend",
        "client": "npm run serve --prefix ../client",
        "dev": "concurrently --kill-others  \"npm run client\" \"npm run server_dev\""
    },
